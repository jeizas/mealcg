<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN" "http://www.tuckey.org/res/dtds/urlrewrite4.0.dtd">
<urlrewrite>
	<rule>
		<!-- 首页 -->
		<from>^(\/?)$</from>
		<to>index</to>
	</rule>
	<rule>
		<from>^/index\.(html|do|action)?$</from>
		<to>hello</to>
	</rule>
	<rule>
		<note>登录</note>
		<from>^/alogin$</from>
		<to>login</to>
	</rule>
	<rule>
		<note>注册</note>
		<from>^/aregest$</from>
		<to>reg</to>
	</rule>
</urlrewrite>