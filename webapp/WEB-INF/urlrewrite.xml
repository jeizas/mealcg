<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN" "http://www.tuckey.org/res/dtds/urlrewrite4.0.dtd">
<urlrewrite>
	<rule>
		<!-- 首页 -->
		<from>^(\/?)$</from>
		<to>inde</to>
	</rule>
	<rule>
		<from>index\.(html|do|action)?$</from>
		<to>inde</to>
	</rule>
</urlrewrite>